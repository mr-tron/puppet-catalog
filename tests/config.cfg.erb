<%- if @backend_urls %>
<% @backend_urls.each do |index,val| -%>
backend<%= index %>=<%= val['protocol'] %>://<%= val['host'] %>:<%= val['port'] %><%= val['url'] %>
<% end -%>
<% end -%>

